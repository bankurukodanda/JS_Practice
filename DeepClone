function deepClone(test){
    var tempObj={};
    if(test == null || typeof test == Object)
        return test;
    if(Array.isArray(test))
        return test.map(deepClone)
    for(key  in test){
        tempObj[key] = deepClone(test[key]);
    }
    return tempObj;
} 

var obj={
    "one":1,
    "two":2,
    "three":{
        "first":1,
        "second":2
    }
}
// var obj1 = obj;
var obj1 = deepClone(obj);
obj1["one"]=5;
obj1["three"]["first"]=7;
console.log(obj);
